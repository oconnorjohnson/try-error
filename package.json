{
  "name": "try-error",
  "version": "0.0.1-alpha.1",
  "description": "Lightweight, progressive, type-safe error handling for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && pnpm -r build",
    "test": "jest && pnpm -r test",
    "test:watch": "jest --watch",
    "test:types": "tsc --noEmit",
    "dev": "tsc --watch",
    "benchmark": "ts-node benchmark/performance.ts",
    "clean": "rm -rf dist && pnpm -r clean"
  },
  "keywords": [
    "typescript",
    "error-handling",
    "errors",
    "type-safe",
    "try-catch",
    "result",
    "either"
  ],
  "author": "Daniel Johnson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danieljohnson/try-error"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "try-error": "workspace:*"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "tsx": "4.19.4"
  }
}
